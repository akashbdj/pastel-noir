  "tokenColors": [
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#F07171"
      }
    },
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#b4cce1"
      }
    },
    {
      "scope": [
        "variable",
        "punctuation.definition.variable",
        "support.variable.property.dom",
        "support.variable.dom",
        "support.variable.property",
        "punctuation.separator.parameter",
        "string.interpolated.pug variable"
      ],
      "settings": {
        "foreground": "#b4cce1"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#465966"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#799889"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#6983A5"
      }
    },
    {
      "scope": ["keyword", "constant.language.import-export-all"],
      "settings": {
        "foreground": "#b1729a"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type.primitive",
        "support.type.builtin",
        "meta.type.annotation entity.name.type",
        "meta.type.parameters entity.name.type"
      ],
      "settings": {
        "foreground": "#6983A5"
      }
    },
    {
      "scope": [
        "keyword.control",
        "keyword.operator",
        "storage",
        "support.type",
        "keyword.operator.expression",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#6983A5"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.class",
        "support.function",
        "new.expr entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#a89c94"
      }
    },
    {
      "scope": [
        "punctuation.definition.typeparameters",
        "keyword.operator.type",
        "keyword.operator.optional",
        "punctuation.definition.template-expression",
        "source.tsx punctuation.section.embedded",
        "source.jsx punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#B4CCE1"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#B4CCE1"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#7DBFEF"
      }
    },
    {
      "scope": [
        "variable.parameter",
        "parameter.variable",
        "meta.function.parameter variable",
        "source.rust meta.type_params.rust"
      ],
      "settings": {
        "foreground": "#B4CCE1"
      }
    },
    {
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#8192a1"
      }
    },
    {
      "scope": ["variable.language.this", "variable.language.special.self"],
      "settings": {
        "foreground": "#6983A5"
      }
    },
    {
      "scope": "comment.block.documentation entity.name.type",
      "settings": {
        "foreground": "#B4CCE1"
      }
    },
    {
      "scope": "variable.language.super",
      "settings": {
        "foreground": "#a89c94"
      }
    },
    {
      "scope": [
        "meta.tag.metadata.doctype entity.name.tag",
        "meta.tag.metadata.doctype punctuation.definition.tag",
        "meta.tag.metadata.doctype string",
        "meta.tag.metadata.doctype entity.other.attribute-name.html",
        "meta.tag.sgml.doctype"
      ],
      "settings": {
        "foreground": "#8192a1"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#6983A5"
      }
    },
    {
      "scope": "meta.tag string",
      "settings": {
        "foreground": "#799889"
      }
    },
    {
      "scope": [
        "meta.tag entity.other.attribute-name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#b1729a"
      }
    },
    {
      "scope": ["constant.character.entity", "punctuation.definition.entity"],
      "settings": {
        "foreground": "#7DBFEF"
      }
    },
    {
      "scope": ["entity.name.section.markdown", "markup.heading.setext"],
      "settings": {
        "foreground": "#b1729a"
      }
    },
    {
      "scope": "punctuation.definition.list",
      "settings": {
        "foreground": "#b1729a"
      }
    },
    {
      "scope": "meta.separator.markdown",
      "settings": {
        "foreground": "#b1729a"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#6983A5"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.link punctuation.definition.string",
        "meta.image punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#8192a1"
      }
    },
    {
      "scope": [
        "link",
        "markup.underline.link",
        "constant.other.reference.link.markdown"
      ],
      "settings": {
        "foreground": "#a89c94"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#7DBFEF"
      }
    },
    {
      "scope": ["entity.name.tag.css", "entity.name.tag.wildcard"],
      "settings": {
        "foreground": "#269299"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.class",
        "entity.other.attribute-name punctuation.definition.entity"
      ],
      "settings": {
        "foreground": "#b1729a"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#6983A5"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.id punctuation.definition.entity"
      ],
      "settings": {
        "foreground": "#a89c94"
      }
    },
    {
      "scope": [
        "source.css constant.numeric",
        "source.less constant.numeric",
        "source.scss constant.numeric"
      ],
      "settings": {
        "foreground": "#269299"
      }
    },
    {
      "scope": ["meta.property-name", "support.type.property-name"],
      "settings": {
        "foreground": "#7DBFEF"
      }
    },
    {
      "scope": [
        "meta.property-value",
        "meta.property-value constant.other",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#a89c94"
      }
    },
    {
      "scope": "variable.parameter.url",
      "settings": {
        "foreground": "#7DBFEF"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": {
        "foreground": "#b1729a"
      }
    },
    {
      "scope": "entity.name.section",
      "settings": {
        "foreground": "#a89c94"
      }
    },
    {
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#b1729a"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#B4CCE1"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#B4CCE1"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#F07171"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#B4CCE1"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#6983A5"
      }
    }
  ]
}
